<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd"
      no-conversation-view-id="/admin/Home.xhtml"
      login-required="true">
   
   <begin-conversation join="true"/>
   
   <action execute="#{capabilityHome.wire}"/>
   
   <param name="capabilityFrom"/>
   <param name="capabilityId" value="#{capabilityHome.capabilityId}"/>


   <navigation from-action="#{capabilityHome.persist}">
       <end-conversation/>
       <redirect view-id="/admin/Capability.xhtml"/>
   </navigation>
   
   <navigation from-action="#{capabilityHome.update}">
       <end-conversation/>
       <redirect view-id="/admin/Capability.xhtml"/>
   </navigation>
   
   <navigation from-action="#{capabilityHome.remove}">
       <end-conversation/>
       <redirect view-id="/admin/CapabilityList.xhtml"/>
   </navigation>
   
</page>
